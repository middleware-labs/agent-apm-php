{
  "name": "middleware/agent-apm-php",
  "description": "Agent APM for PHP",
  "keywords": ["apm", "php", "agent", "middleware", "tracing", "opentelemetry", "otel", "instrumentation"],
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Middleware Developers",
      "email": "dev@middleware.io",
      "homepage": "https://middleware.io/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "open-telemetry/context": "1.0.0beta2",
    "open-telemetry/sem-conv": "1.0.0beta2",
    "open-telemetry/opentelemetry": "1.0.0beta2",
    "symfony/http-client": "6.3.x-dev",
    "guzzlehttp/promises": "^1.5@dev",
    "php-http/message-factory": "^1.0@dev",
    "nyholm/psr7": "^1.5",
    "psr/http-client": "1.0.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Middleware\\AgentApmPhp\\": ""
    }
  },
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
