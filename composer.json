{
  "name": "middleware/agent-apm-php",
  "description": "Agent APM for PHP",
  "keywords": ["apm", "php", "agent", "middleware", "tracing", "opentelemetry", "otel", "instrumentation"],
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Middleware Developers",
      "email": "dev@middleware.io",
      "homepage": "https://middleware.io/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "open-telemetry/sdk": "^1.0",
    "symfony/http-client": "^6.3",
    "guzzlehttp/promises": "^2.0",
    "php-http/message-factory": "^1.1",
    "nyholm/psr7": "^1.8",
    "open-telemetry/api": "^1.0",
    "open-telemetry/exporter-otlp": "^1.0",
    "open-telemetry/opentelemetry-auto-psr15": "^1.0",
    "psr/http-client": "^1.0",
    "google/protobuf": "^3.25",
    "php-http/httplug": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "Middleware\\AgentApmPhp\\": ""
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "tbachert/spi": true
    }
  }
}
